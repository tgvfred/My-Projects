<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:sal="http://sales.accommodation.transferobject.booking.dreams.wdw.com">
   <soapenv:Header/>
   <soapenv:Body>
      <sal:fulFillPurchaseRequest>
         <pointsPurchaseRequest>
            <purchaseDate>?</purchaseDate>
            <purchaseValue>?</purchaseValue>
            <!--Optional:-->
            <purchasedPointType>?</purchasedPointType>
            <!--Optional:-->
            <productCode>?</productCode>
            <!--Optional:-->
            <productName>?</productName>
            <fulfillmentDate>?</fulfillmentDate>
            <travelPlanId>?</travelPlanId>
            <!--Optional:-->
            <overriddenErrors>?</overriddenErrors>
            <useMonth>?</useMonth>
            <purchaseFor>
               <membershipId>?</membershipId>
               <facility>
                  <!--Optional:-->
                  <enterpriseFacilityId>?</enterpriseFacilityId>
                  <resortCode>?</resortCode>
                  <resortType>?</resortType>
               </facility>
               <contractIdentifier>
                  <!--Optional:-->
                  <contractId>?</contractId>
                  <contractSequence>?</contractSequence>
               </contractIdentifier>
            </purchaseFor>
            <!--Zero or more repetitions:-->
