<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="AccommodationModule_Suite" parallel="methods" data-provider-thread-count="5" thread-count="10" configfailurepolicy="continue">
	<parameter name="environment" value="${environment}" />
	<test name="AccommodationModule_gathering" parallel="methods" thread-count="1">
		<groups>
			<run>
				<exclude name="presmoke"/>
				<exclude name="smoke"/>					
				<exclude name="dev"/>
				<!-- <exclude name="debug"/> -->
				<include name="gathering"></include>
				<exclude name="example"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.accommodationModule.*"></package>
		</packages>
	</test>
	<test name="AccommodationModule_tpv3" parallel="methods" thread-count="2">
		<groups>
			<run>
				<exclude name="presmoke"/>
				<exclude name="smoke"/>					
				<exclude name="dev"/>
				<!-- <exclude name="debug"/> -->
				<include name="tpv3"></include>
				<exclude name="gathering"></exclude>
				<exclude name="example"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.accommodationModule.*"></package>
		</packages>
	</test>
	<test name="AccommodationModule_checkout" parallel="methods" thread-count="2">
		<groups>
			<run>
				<exclude name="presmoke"/>
				<exclude name="smoke"/>					
				<exclude name="dev"/>
				<!-- <exclude name="debug"/> -->
				<include name="checkout"></include>
				<exclude name="tpv3"></exclude>
				<exclude name="gathering"></exclude>
				<exclude name="example"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.accommodationModule.*"></package>
		</packages>
	</test>
	<test name="AccommodationModule_reinstate" parallel="methods" thread-count="2">
		<groups>
			<run>
				<exclude name="presmoke"/>
				<exclude name="smoke"/>					
				<exclude name="dev"/>
				<!-- <exclude name="debug"/> -->
				<include name="reinstate"></include>
				<exclude name="tpv3"></exclude>
				<exclude name="gathering"></exclude>
				<exclude name="example"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.accommodationModule.*"></package>
		</packages>
	</test>
	<test name="AccommodationModule">
		<groups>
			<run>
				<exclude name="presmoke"/>
				<exclude name="smoke"/>					
				<exclude name="dev"/>
				<!-- <exclude name="debug"/> -->
				<exclude name="getoptions"></exclude>
				<exclude name="checkout"></exclude>
				<exclude name="reinstate"></exclude>
				<exclude name="tpv3"></exclude>
				<exclude name="gathering"></exclude>
				<exclude name="example"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.accommodationModule.*"></package>
		</packages>
	</test>
</suite>