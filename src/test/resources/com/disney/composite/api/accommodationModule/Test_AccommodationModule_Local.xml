<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ccommodationModule_Suite" parallel="methods" thread-count="20" configfailurepolicy="continue">
	<parameter name="environment" value="latest" />
