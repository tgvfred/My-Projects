	<parameter name="localHostUrlList" value="http://fldcvdsla16120.wdw.disney.com:9081/Accommodation/AccommodationSalesServicePort;http://fldcvdsla16120.wdw.disney.com:9081/Accommodation/AccommodationSalesComponentServicePort" /> --> 
	
	<!-- <parameter name="darksideServiceList" value="AccommodationSalesServicePort;AccommodationSalesComponentServicePort;REST_AccommodationS?alesService" /> -->
	<test name="AccommodationModule_tpv3" parallel="methods" thread-count="1">
		<groups>
			<run>					
				<exclude name="dev"/>
				<!-- <exclude name="debug"/> -->
				<include name="gathering"></include>
				<include name="tpv3"></include>
				<include name="GCAL"></include>
				<exclude name="example"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.accommodationModule.*"></package>
