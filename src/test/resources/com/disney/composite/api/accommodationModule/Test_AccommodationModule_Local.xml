	<test name="AccommodationModule">
		<groups>
			<run>
				<exclude name="presmoke"/>
				<exclude name="smoke"/>					
				<exclude name="dev"/>
				<!-- <exclude name="debug"/> -->
				<exclude name="getoptions"></exclude>
				<exclude name="checkout"></exclude>
				<exclude name="reinstate"></exclude>
				<exclude name="tpv3"></exclude>
				<exclude name="gathering"></exclude>
				<exclude name="GCAL"></exclude>
				<exclude name="example"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.accommodationModule.*"></package>
		</packages>
