<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test_DiningCompensation_Suite" parallel="methods" thread-count="20" configfailurepolicy="continue">
	<parameter name="environment" value="${environment}" />
	<test name="Test_DiningCompensation">
		<groups>
			<run>
				<include name="compensation"></include>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.diningModule.*"></package>
		</packages>
	</test>
</suite>