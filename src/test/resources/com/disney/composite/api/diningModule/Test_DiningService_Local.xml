<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ShowDiningService_Suite" parallel="methods" thread-count="100"  configfailurepolicy="continue">
	<parameter name="environment" value="Latest_CM" />
	<test name="ShowDiningService">
		<groups>
			<run>
				<include name="dining" />
				<exclude name="compensation"></exclude>
				<exclude name="dev"  />
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.*"></package>
		</packages>
	</test>
</suite>