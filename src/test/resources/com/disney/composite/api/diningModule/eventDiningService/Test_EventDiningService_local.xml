<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="EventDiningService_Suite" parallel="methods" thread-count="10" configfailurepolicy="continue">
	<parameter name="environment" value="Shadow" />
	<test name="EventDiningService">
		<groups>
			<run>
				<exclude name="compensation"></exclude>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.diningModule.eventDiningService.*"></package>
		</packages>
	</test>
	<test name="EventDiningService_WithCompensationValidation">
		<groups>
			<run>
				<include name="compensation"></include>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.api.diningModule.eventDiningService.*"></package>
		</packages>
	</test>
</suite>