<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="API_Suite" parallel="classes" thread-count="100"  data-provider-thread-count="10" >
	<!-- <parameter name="runLocation" value="${runLocation}" /> -->
	
	<parameter name="environment" value="sleepy" />
	<test name="Regression">
		<groups>
			<run>
				<include name="api"/>  
				<exclude name="dvc"/>
			</run>
		</groups>
		<packages>
			<package name="com.disney.composite.*"/>
		</packages>
	</test> <!-- Test -->
</suite> <!-- Suite -->
